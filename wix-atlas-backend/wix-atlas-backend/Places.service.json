      "docs": {
        "summary": "Once you have a search_id from a autocomplete Search, you can request more details about a particular establishment or point of interest by initiating a get place request.",
        "description": [],
        "examples": [
          {
            "title": "getPlace example",
            "body": [
              "",
              "    ",
              "    import { places } from 'wix-atlas-backend';",
              "  ",
              "    ",
              "    async function getPlace(searchId, options) {",
              "      try {",
              "        const result = await places.getPlace(searchId, options);",
              "",
              "        return result;",
              "        } catch (error) {",
              "          console.error(error);",
              "          // Handle the error",
              "        }",
              "    }",
              "  "
            ]
          }
        ]
