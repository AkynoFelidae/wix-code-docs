    {
      "name": "reverseGeocoding",
      "params": [
        {
          "name": "addressLocation",
          "type": [
            {
              "referenceType": "wix-atlas-backend.Location.AddressLocation"
            }
          ],
          "doc": "The latitude and longitude values specifying the location for which you wish to obtain the closest, human-readable address.",
          "required": true
        }
      ],
      "requiredFields": [
        "addressLocation",
        "addressLocation.latitude",
        "addressLocation.longitude"
      ],
      "ret": {
        "type": [
          {
            "complexType": {
              "nativeType": "Promise",
              "typeParams": [
                {
                  "referenceType": "wix-atlas-backend.Location.ReverseGeocodingResponse"
                }
              ]
            }
          }
        ],
        "doc": null
      },
      "docs": {
        "summary": "A Search request takes 'latitude' and 'longitude' values as an input and returns a list of 'Address' objects.",
        "description": [],
        "examples": [
          {
            "title": "reverseGeocoding example",
            "body": [
              "",
              "    ",
              "    import { location } from 'wix-atlas-backend';",
              "  ",
              "    ",
              "    async function reverseGeocoding(addressLocation) {",
              "      try {",
              "        const result = await location.reverseGeocoding(addressLocation);",
              "",
              "        return result;",
              "        } catch (error) {",
              "          console.error(error);",
              "          // Handle the error",
              "        }",
              "    }",
              "  "
            ]
          }
        ]
      },
      "isVeloEvent": false
    },
